.header {
  height: 50px;
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 9999;
  border-bottom: 1px solid #eee;
  background: #fff;
}
.header .headerL {
  position: absolute;
  left: 0;
  top: 0;
  height: 50vw;
  padding-left: 15px;
}

.header .headerL .img {
  width: 16px;
  height: 21px;
  padding-top: 16px;
  float: left;
}
.header .goback span {
  float: left;
  line-height: 50px;
  font-size: 12px;
  color: #333;
  margin-left: 3px;
}
.header .headerC {
  width: 100%;
  height: 50px;
  line-height: 50px;
  text-align: center;
}
.header .headerC p {
  text-align: center;
  font-size: 16px;
  color: #333;
  letter-spacing: 1px;
}
.header .headerR {
  position: absolute;
  right: 0;
  top: 0;
  height: 50px;
  padding-right: 15px;
}
.header .headerR .dots {
  float: right;
  width: 50px;
  height: 50px;
  text-align: right;
}
.header .headerR .dots i {
  width: 4px;
  height: 4px;
  background: #333;
  border-radius: 50%;
  display: inline-block;
  position: relative;
  top: 12px;
}
.header .headerR .dots i:nth-child(2) {
  margin: 0 5px;
}